{
  "name": "apollo-link-timeout",
  "version": "1.4.1",
  "description": "Abort requests that take longer than a specified timeout period",
  "dependencies": {
    "@apollo/client": "3.1.3",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsdoc": "^30.2.1",
    "eslint-plugin-no-null": "^1.0.2",
    "graphql": "^15.3.0",
    "jest": "^26.3.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7"
  },
  "main": "lib/timeoutLink.js",
  "types": "lib/timeoutLink.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelchiche/apollo-link-timeout"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/*.ts* src/**/*.ts*",
    "test": "jest",
    "prepublish": "npm run test && npm run build",
    "deploy": "npm run publish && git push"
  },
  "keywords": [
    "graphql",
    "apollo",
    "apollo-link",
    "timeout"
  ],
  "author": "Dustin Callaway <drcallaway@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  }
}
